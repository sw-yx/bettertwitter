{
  "extends": "./paths.json",
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": [
      "es6",
      "es7",
      "dom"
    ],
    "jsx": "preserve"
  },
  "exclude": [
    "src/setupProxy.js",
    "src/lambda"
  ],
  "include": [
    "src"
  ]
}
